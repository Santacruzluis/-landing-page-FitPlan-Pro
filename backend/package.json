{
  "name": "fitplan-pro-backend",
  "version": "1.0.0",
  "description": "Backend API for FitPlan Pro - AI-powered fitness application",
  "author": "FitPlan Pro Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "build:prod": "npx tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "yarn typeorm -- migration:generate -d src/config/database.config.ts",
    "migration:run": "yarn typeorm -- migration:run -d src/config/database.config.ts",
    "migration:revert": "yarn typeorm -- migration:revert -d src/config/database.config.ts",
    "db:seed": "yarn ts-node src/database/seeds/index.ts",
    "db:reset": "yarn migration:revert && yarn migration:run && yarn db:seed",
    "render:build": "yarn install && yarn build:prod",
    "render:start": "yarn migration:run && yarn start:prod",
    "seed": "ts-node src/database/seeds/index.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/config": "^3.1.1",
    "@nestjs/throttler": "^5.0.1",
    "@nestjs/cli": "^10.0.0",
    "typeorm": "^0.3.17",
    "pg": "^8.11.3",
    "bcryptjs": "^2.4.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "swagger-ui-express": "^5.0.0",
    "openai": "^4.28.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/bcryptjs": "^2.4.4",
    "@types/pg": "^8.10.7",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.89.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  }
}
